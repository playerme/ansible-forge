{
    "presets": ["react", "es2015", "stage-0"],
    "plugins": ["transform-decorators-legacy"],
    "env": {
        "development": {
            "plugins": [
                ["transform-object-rest-spread"],
                ["react-transform", {
                    "transforms": [
                        {
                            "transform": "react-transform-hmr",
                            "imports": ["react"],
                            "locals": ["module"]
                        },
                        {
                            "transform": "react-transform-catch-errors",
                            "imports": ["react", "redbox-react"]
                        }
                    ]
                }]
            ]
        }
    }
}
